{
  "name": "palindromes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist build",
    "preview": "npm run build && npm run start",
    "build:migrate": "npm run db:migrate && npm run build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:stu#dio": "drizzle-kit studio",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "scrape-palindromes": "dotenv -e .env.local tsx src/scripts/scrape-palindromes.ts",
    "scrape:test": "npm run scrape-palindromes -- --test --batch-size 16 --concurrency 16",
    "scrape:test-fast": "npm run scrape-palindromes -- --test --batch-size 50 --concurrency 10",
    "scrape:status": "npm run scrape-palindromes -- --status",
    "scrape:init": "npm run scrape-palindromes -- --init-only"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-table": "^8.21.3",
    "@types/jsdom": "^21.1.7",
    "@types/pg": "^8.15.5",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "jsdom": "^27.0.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
